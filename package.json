{
  "name": "anq-server-select",
  "version": "0.0.18",
  "description": "A powerful, customizable server select component for Quasar Framework and Vue 3 applications. Features include server selection, validation, and seamless integration with Quasar's design system.",
  "type": "module",
  "main": "./dist/anq-server-select.umd.cjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "module": "./dist/anq-server-select.js",
  "exports": {
    ".": {
      "import": "./dist/anq-server-select.js",
      "require": "./dist/anq-server-select.umd.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@quasar/extras": "^1.16.12",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "quasar": "^2.16.11",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.33.0",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vue-tsc": "^2.0.29"
  },
  "keywords": [
    "anq-server-select",
    "server select",
    "quasar server select",
    "vue server select",
    "quasar framework",
    "vue 3",
    "server selection",
    "server input component",
    "quasar components",
    "vue components",
    "responsive server select",
    "customizable server select",
    "typescript server select"
  ],
  "author": "aymane_nahji",
  "repository": {
    "type": "git",
    "url": "https://github.com/AymaneNahji/anq-server-select"
  },
  "bugs": {
    "url": "https://github.com/AymaneNahji/anq-server-select/issues"
  },
  "homepage": "https://github.com/AymaneNahji/anq-server-select#readme",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "quasar": "^2.0.0",
    "vue": "^3.0.0"
  }
}
